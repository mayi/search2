{
  "description": "Create a good search habit by yourself !",
  "author": "wurenny, fireant",
  "web_accessible_resources": [
    {
      "resources": [
		"inc/comm.js",
		"inc/util.js",
		"inc/prototype-func.js",
		"opt/init-data.js",
        "cts/contentScript.js",
        "css/css.css",
        "css/pull.css",
        "more/more.css",
        "opt/options.html"
      ],
      "matches": ["*://*/*"]
    }
  ],
  "background": {
    "service_worker": "bg/service_worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file:///*"],
      "include_globs": ["http:*", "https:*"],
      "js": ["inc/prototype-func.js", "inc/comm.js", "cts/contentScript.js"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "default_locale": "zh_CN",
  "icons": {
    "16": "img/logo16.png",
    "32": "img/logo32.png",
    "48": "img/logo48.png",
    "96": "img/logo96.png",
    "128": "img/logo128.png",
    "256": "img/logo256.png"
  },
  "minimum_chrome_version": "92",
  "manifest_version": 3,
  "name": "__MSG_app_title__",
  "options_page": "opt/options.html",
  "permissions": ["storage", "contextMenus"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "version": "1.1.12"
}
